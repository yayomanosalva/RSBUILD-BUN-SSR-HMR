{
  "name": "rsbuild-bun-ssr-hmr",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "check": "biome check --write",
    "dev": "NODE_ENV=development rsbuild dev --open",
    "build": "NODE_ENV=production rsbuild build",
    "buildsss": "rsbuild build",
    "build:client": "rsbuild build --env client",
    "build:server": "rsbuild build --env server",
    "build:all": "bun run build:client && bun run build:server",
    "start": "bun run src/server.ts",
    "format": "prettier --write .",
    "lint": "eslint .",
    "preview": "rsbuild preview",
    "hmr:watch": "bun --watch src/server.ts"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/themes": "^3.2.1",
    "@rsbuild/plugin-css-minimizer": "^1.0.2",
    "@swc/plugin-emotion": "^9.0.0",
    "@swc/plugin-styled-components": "^7.1.0",
    "@tanstack/react-query": "^5.71.0",
    "@tanstack/react-query-devtools": "^5.71.0",
    "axios": "^1.8.4",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.4.1",
    "react-server-dom-webpack": "^19.1.0",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.23.0",
    "@rsbuild/core": "^1.3.0",
    "@rsbuild/plugin-react": "^1.1.1",
    "@tailwindcss/postcss": "^4.0.17",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "bun-types": "^1.2.7",
    "eslint": "^9.23.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "sass": "^1.86.0",
    "tailwindcss": "^4.0.17",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  }
}
